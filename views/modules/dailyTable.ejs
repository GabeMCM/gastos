<table class="table" id="<%= obj%>">
  <thead>
    <tr>
      <th>Descrição</th>
      <th>Valor</th>
      <th>Tipo</th>
    </tr>
  </thead>
    <tbody>
      <% list[obj].forEach((data) => { %>
        <tr>
          <td><%= data.descricao %></td>
          <td><%= data.valor %></td>
          <% if(obj === "daily") { %>
            <td>Diária</td>
          <% } %>
        </tr>
      <% }) %>
        <tr>
          <form action="/save/<%=obj%>Account" method="post">
            <td>
              <input name="descricao" type="text" placeholder="desc">
            </td>
            <td>
              <input name="valor" type="number" step="0.01" placeholder="valor1">
            </td>
            <td>
              <button id="add-data3" type="submit">Adicionar</button>
            </td>
          </form>
        </tr>
    </tbody>
</table>